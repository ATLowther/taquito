{
  "name": "@taquito/contract-type-generator",
  "version": "8.0.6-beta.0",
  "description": "Contract Typescript Type Generator",
  "keywords": [
    "tezos",
    "blockchain",
    "types"
  ],
  "main": "./dist/taquito-contract-type-generator.umd.js",
  "module": "./dist/taquito-contract-type-generator.js",
  "typings": "./dist/types/taquito-contract-type-generator.d.ts",
  "files": [
    "dist",
    "signature.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Rick Love <rick@toldpro.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecadlabs/taquito.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "bin": {
    "contract-type-generator": "bin/contract-type-generator.js"
  },
  "scripts": {
    "examples": "npm run example && npm run example-json && npm run example-simple && npm run example-file && npm run example-local",
    "example": "contract-type-generator --g ./example/contracts ./example/types",
    "example-json": "contract-type-generator --json --g ./example/contracts ./example/types",
    "example-simple": "contract-type-generator --types simple --g ./example/contracts ./example/types-simple",
    "example-file": "contract-type-generator --types file --g ./example/contracts ./example/types-file",
    "example-local": "contract-type-generator --types local --g ./example/contracts ./example/types-local",
    "test-generate": "contract-type-generator --g ../taquito-michel-codec/test/ ./test/michel-codec-test-contract-types/",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts'",
    "precommit": "lint-staged",
    "prebuild": "rimraf dist",
    "build": "tsc --project ./tsconfig.prod.json --module commonjs && rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "test": "npm run examples && npx jest"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^@taquito/utils$": "<rootDir>/../taquito-utils/src/taquito-utils.ts"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "dependencies": {
    "@taquito/michel-codec": "^8.0.6-beta.0",
    "@taquito/taquito": "^8.0.6-beta.0",
    "bignumber.js": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "rimraf": "^3.0.2",
    "rollup": "^2.42.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  }
}